{
    "name": "puwnz/google-maps-package",
    "description": "Laravel wrapper around google maps library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pawndev",
            "email": "coquelet.c@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3.*",
        "illuminate/support": "^7.16",
        "puwnz/google-maps-lib": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Puwnz\\GoogleMapsPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": "Puwnz\\GoogleMapsPackage\\GoogleMapsServiceProvider"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "phpcs": [
            "php vendor/bin/php-cs-fixer fix --dry-run --diff  --allow-risky=yes"
        ]
    }
}
